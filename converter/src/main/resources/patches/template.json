{
  "formatVersion": 1,
  "mainClass": "io.github.zekerzhayard.forgewrapper.installer.Main",
  "minecraftArguments": "",
  "name": "Forge",
  "requires": [
    {
      "equals": "{VERSION}",
      "uid": "net.minecraft"
    }
  ],
  "type": "release",
  "uid": "{FORGE_GROUP}",
  "version": "{FORGE_VERSION}",
  "mavenFiles": [
    {
      "name": "{FORGE_GROUP}:forge:{VERSION}-{FORGE_VERSION}:installer",
      "url": "{MAVEN_URL}"
    }
  ],
  "libraries": [
    {
      "name": "io.github.zekerzhayard:ForgeWrapper:${version}",
      "MMC-hint": "local",
      "MMC-filename": "ForgeWrapper-${version}.jar"
    }
  ]
}
